# Kiero 로컬 개발 환경 변수
# 이 파일을 복사하여 .env 파일을 생성하세요
# cp .env.example .env

# Database - Local MySQL
DB_URL=jdbc:mysql://localhost:3306/kiero?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=Asia/Seoul
DB_USERNAME=root
DB_PASSWORD=your-local-password

# AWS S3
AWS_S3_BUCKET=kiero-bucket
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY=YOUR_ACCESS_KEY
AWS_SECRET_KEY=YOUR_SECRET_KEY
