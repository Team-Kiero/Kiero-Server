# Kiero 환경 변수 설정

# Database
DB_URL=jdbc:mysql://localhost:3306/kiero?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=Asia/Seoul
DB_HOST=localhost
DB_NAME=kiero
DB_USERNAME=root
DB_PASSWORD=your-database-password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# AWS S3
AWS_S3_BUCKET=kiero-bucket
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY=YOUR_AWS_ACCESS_KEY
AWS_SECRET_KEY=YOUR_AWS_SECRET_KEY

# Kakao OAuth2
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret

# JWT
JWT_SECRET=your-jwt-secret-key-min-256-bits-12345678901234567890123456789012
JWT_ACCESS_TOKEN_EXPIRE_TIME=3600000
JWT_REFRESH_TOKEN_EXPIRE_TIME=604800000

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# OpenAI (Spring AI)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Sentry
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Open API (공공데이터)
OPEN_API_SERVICE_KEY=your-open-api-service-key

# Grafana
GRAFANA_ADMIN_PASSWORD=your-grafana-admin-password

# NocoDB
NOCODB_DB_HOST=your-rds-endpoint.ap-northeast-2.rds.amazonaws.com
NOCODB_DB_NAME=kiero
NOCODB_JWT_SECRET=your-nocodb-jwt-secret-key-min-32-chars
NOCODB_ADMIN_EMAIL=admin@example.com
NOCODB_ADMIN_PASSWORD=your-nocodb-admin-password