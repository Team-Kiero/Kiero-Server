# Kiero 환경 변수 설정
# 이 파일을 복사하여 실제 환경변수 파일을 생성하세요
# 로컬: cp .env.example .env
# 개발: cp .env.example .env.dev
# 운영: cp .env.example .env.prod

# Database
DB_URL=jdbc:mysql://localhost:3306/kiero?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=Asia/Seoul
DB_USERNAME=root
DB_PASSWORD=your-database-password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# AWS S3
AWS_S3_BUCKET=kiero-bucket
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY=YOUR_AWS_ACCESS_KEY
AWS_SECRET_KEY=YOUR_AWS_SECRET_KEY

# Kakao OAuth2
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret

# JWT
JWT_SECRET=your-jwt-secret-key-min-256-bits-12345678901234567890123456789012
JWT_ACCESS_TOKEN_EXPIRE_TIME=3600000
JWT_REFRESH_TOKEN_EXPIRE_TIME=604800000

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# OpenAI (Spring AI)
OPENAI_API_KEY=sk-your-openai-api-key-here
